{
    "name": "nacosvel/2fa",
    "description": "Community-driven PHP library for generating OTPs (HOTP/TOTP), RFC-compliant and compatible with Google Authenticator.",
    "keywords": [
        "Google Authenticator",
        "2fa",
        "otps",
        "hotp",
        "totp",
        "RFC 4226",
        "RFC 6238"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/nacosvel/2fa",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Nacosvel\\Authenticator\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
